package com.jasminesloan.packages;

public enum Info {
	
	iMac("21.5 inch", "$1299", "2.7GHz dual-core Intel Core i5", "1TB"),
	MacBookPro("13 inch", "$1199", "2.5GHz dual-core Intel Core i5", "500GB"),
	MacBookAir("11 inch", "$999", "1.3GHz dual-core Intel Core i5", "128GB"),
	iPad("9.5 inch", "$499", "802.11a/b/g/n Wi‑Fi (802.11n 2.4GHz and 5GHz)", "16GB"),
	Mini("7.87 inch", "$329", "802.11a/b/g/n Wi‑Fi (802.11n 2.4GHz and 5GHz)", "16GB"),
	iPod("4 inch", "$229", "802.11a/b/g/n Wi-Fi (802.11n 2.4GHz and 5GHz)", "16GB"),
	iPhone("4.87 inch", "$199", "802.11a/b/g/n Wi-Fi (802.11n 2.4GHz and 5GHz)", "16GB"),
	
	
	private final String screenSize;
	private final String price;
	private final String processor;
	private final String storage; 
	
	private Info(String screenSize, String price, String processor, String storage){
		this.screenSize = screenSize;
		this.price = price;
		this.processor = processor;
		this.storage = storage;
	}
	
	public String setScreenSize(){
		return screenSize;
	}
	
	public String setPrice(){
		return price;
	}
	
	public String setProcessor(){
		return processor;
	}
	
	public String setStorage(){
		return storage;
	}
	
}
